{
  "name": "gioastro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 astro dev",
    "build": "NODE_TLS_REJECT_UNAUTHORIZED=0 astro build",
    "preview": "NODE_TLS_REJECT_UNAUTHORIZED=0 astro preview",
    "astro": "astro",
    "build:cf": "ASTRO_ADAPTER=cloudflare astro build",
    "dev:cf": "npm run build:cf && wrangler pages dev ./dist",
    "deploy:prod": "npm run build:cf && npx wrangler pages deploy",
    "tail:prod": "npx wrangler pages deployment tail --project-name gioastro"
  },
  "dependencies": {
    "@astrojs/node": "^9.5.4",
    "@tailwindcss/vite": "^4.1.13",
    "astro": "^5.17.3",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "wrangler": "^4.68.1"
  }
}
